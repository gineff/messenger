{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "airbnb", "prettier"],
  "overrides": [
    {
      "files": ["*.js"],
      "rules": {
        "react/no-unused-state": "off",
        "react/prop-types": "off"
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "quotes": ["error", "double"]
  }
}
