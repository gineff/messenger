{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb",
      "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "quotes": [
        "error",
        "double"
      ]
    }
}
